<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="fos_http_cache.proxy_client.symfony"
                 class="FOS\HttpCache\ProxyClient\Symfony">
            <argument type="service" id="fos_http_cache.proxy_client.symfony.http_dispatcher"/>
            <argument>%fos_http_cache.proxy_client.symfony.options%</argument>
        </service>
        <service id="fos_http_cache.symfony.kernel_client"
                 class="FOS\HttpCache\SymfonyCache\KernelClient">
            <argument type="service" id="kernel"/>
        </service>
    </services>

</container>
