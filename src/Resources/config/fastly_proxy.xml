<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="fos_http_cache.test.proxy_server.fastly"
                 class="FOS\HttpCache\Test\Proxy\FastlyProxy">
            <argument on-invalid="ignore">%fos_http_cache.test.proxy_server.fastly.config_file%</argument>
            <call method="setBinary">
                <argument on-invalid="ignore">%fos_http_cache.test.proxy_server.fastly.binary%</argument>
            </call>
            <call method="setPort">
                <argument>%fos_http_cache.test.proxy_server.fastly.port%</argument>
            </call>
            <call method="setIp">
                <argument>%fos_http_cache.test.proxy_server.fastly.ip%</argument>
            </call>
        </service>
    </services>

</container>
